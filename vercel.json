{
  "version": 2,
  "builds": [
    {
      "src": "src/server.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["src/**/*"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/server.js"
    }
  ],
  "projectSettings": {
    "framework": "node",
    "buildCommand": "npm run vercel-build",
    "outputDirectory": "dist"
  }
}
